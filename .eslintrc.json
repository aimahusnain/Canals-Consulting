{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/no-unescaped-entities": [
      "error",
      {
        "forbid": [
          {
            "char": "'",
            "alternative": "&apos;"
          },
          {
            "char": "\"",
            "alternative": "&quot;"
          }
        ]
      }
    ]
  }
}